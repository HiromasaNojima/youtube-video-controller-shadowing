{
  "name": "youtube video controller for shadowing",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "help your practice of shadowing!",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*"],
      "js": ["controller.js"]
    }
  ],
  "background": { "scripts": ["hot-reload.js"] },
  "permissions": ["activeTab"]
}
