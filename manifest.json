{
  "name": "youtube video controller for shadowing",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample Chrome Extension",
  "content_scripts": [{
    "matches": ["https://www.youtube.com/watch?v=*"],
    "js": [
      "controller.js"
    ]
  }],
  "permissions": [
      "activeTab"
  ]
}