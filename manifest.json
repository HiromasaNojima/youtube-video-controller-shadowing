{
  "name": "youtube video controller for shadowing",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "help you practice shadowing on YouTube",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch?v=*"],
      "js": ["controller.js"]
    }
  ],
  "icons": {
    "128": "128.png"
  },
  "permissions": ["activeTab"]
}
